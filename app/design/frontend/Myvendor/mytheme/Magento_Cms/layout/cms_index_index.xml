<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Magento_Cms::css/owl.carousel.min.css"/>
		<css src="Magento_Cms::css/owl.theme.default.css"/>
	</head>
	<!-- Brand Logo Slider Block -->
	<block class="Magento\Cms\Block\Block" name="logo.slider">
	    <arguments>
	        <argument name="block_id" xsi:type="string">logo-slider</argument>
	    </arguments>
	</block>
	<!-- Brand Logo Slider Move -->
	<move element="logo.slider" destination="main" after="-" />

    <body/>
</page>
